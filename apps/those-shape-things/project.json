{
  "name": "those-shape-things",
  "sourceRoot": "apps/those-shape-things/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "those-shape-things:build",
        "port": 5175
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/those-shape-things",
        "configFile": "apps/those-shape-things/vite.config.js"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "those-shape-things:build",
        "port": 4175
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/those-shape-things/**/*.{js,jsx}"]
      }
    },
    "deploy": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "command": "npx gh-pages -d dist/apps/those-shape-things --repo https://github.com/michaelpaulukonis/those-shape-things.git"
      }
    },
    "release-deploy": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx release --projects=those-shape-things",
          "npx gh-pages -d dist/apps/those-shape-things --repo https://github.com/michaelpaulukonis/those-shape-things.git"
        ],
        "parallel": false
      }
    }
  }
}