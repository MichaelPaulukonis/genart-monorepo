{
  "name": "computational-collage",
  "sourceRoot": "apps/computational-collage/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "computational-collage:build",
        "port": 5176
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/computational-collage",
        "configFile": "apps/computational-collage/vite.config.js"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "computational-collage:build",
        "port": 4176
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/computational-collage/**/*.{js,jsx}"]
      }
    },
    "deploy": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "command": "npx gh-pages -d dist/apps/computational-collage --repo https://github.com/michaelpaulukonis/computational-collage.git"
      }
    },
    "release-deploy": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx release --projects=computational-collage",
          "npx gh-pages -d dist/apps/computational-collage --repo https://github.com/michaelpaulukonis/computational-collage.git"
        ],
        "parallel": false
      }
    }
  }
}